                <div id="navbarSupportedContent" class="collapse navbar-collapse">
                    <ul class="navbar-nav ml-auto d-lg-flex align-items-lg-center">
                        <li class="nav-item">
                            <a href="@Url.Action("Index", "Home")" class="nav-link font-weight-bold text-uppercase px-lg-3 py-lg-4 active">Home </a>
                        </li>
                        <li class="nav-item dropdown">
                            <a id="navbarDropdownMenuLink" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="nav-link font-weight-bold text-uppercase px-lg-3 py-lg-4 dropdown-toggle">Admin</a>
                            <div aria-labelledby="navbarDropdownMenuLink" class="dropdown-menu mt-0"><a href="#" class="dropdown-item small text-uppercase">Courses</a><a href="#" class="dropdown-item small text-uppercase">Enrollments</a><a href="#" class="dropdown-item small text-uppercase">Scheduled Classes</a><a href="#" class="dropdown-item small text-uppercase">Students</a><a href="#" class="dropdown-item small text-uppercase">Student Statuses</a></div>
                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("Contact", "Home")" class="nav-link font-weight-bold text-uppercase px-lg-3 py-lg-4">Contact </a>
                        </li>
                        <li class="nav-item mt-4 mt-lg-0">
                            <button type="button" data-toggle="modal" data-target="#loginModal" class="btn btn-outline-primary nav-btn btn-sm"> <i class="fas fa-sign-out-alt mr-2"></i>@Html.Partial("_LoginPartial")</button>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>


    @RenderBody()

